 <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

  <material name="white">
    <color rgba="0.9 0.9 0.9 1"/>
  </material>
  <material name="pure_white">
    <color rgba="1 1 1 1"/>
  </material>
    <material name="clear_gray">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  
  <material name="black">
    <color rgba="0.05 0.05 0.05 0.95"/>
  </material>
  

